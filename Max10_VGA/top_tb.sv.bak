`timescale 1 ns / 100 ps

module tb();
	reg clk = 1'b0;
	
	reg [14:0] v_gpio_d;
	reg [8:1] v_led;
	
	VGA_Controller VGA_Controller_Inst(
		.pixel_clk(clk),
		.gpio_d(v_gpio_d)
	);

	// 25MHz clock
	always #5 clk <= ~clk;
	
	
	initial begin
		$display($time, " Starting the Simulation");
		#1000
		$finish();
	end
	
endmodule
